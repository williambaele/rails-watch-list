
<div class="container mb-4 mt-4">
  <h1>Add a bookmark to your list ?</h1>
<%= simple_form_for [@list, @bookmark] do |f| %>

    <div class="form-inputs">
  <%= f.input :comment %>
  <%= f.association :movie %>    </div>
    <div class="form-actions">
      <%= f.button :submit, class: "btn btn-success"%>
    </div>
  <% end %>
</div>
